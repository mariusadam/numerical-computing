{MPLTEXT 1 0 29 "  for j from 1 to ld[k] do  \n" }{MPLTEXT 1 0 34 "   \+
 for pp from 1 to ld[k]-j+1 do\n" }{MPLTEXT 1 0 27 "      td[ic,pp]:=t
vd[pp]; \n" }{MPLTEXT 1 0 12 "    end do;\n" }{MPLTEXT 1 0 14 "    ic:
=ic+1;\n" }{MPLTEXT 1 0 10 "  end do;\n" }{MPLTEXT 1 0 27 "  z:=[op(z)
,(x[k]$ld[k])];\n" }{MPLTEXT 1 0 8 "end do;\n" }{MPLTEXT 1 0 22 "for k
 from 2 to sz do\n" }{MPLTEXT 1 0 30 "    for j from 1 to sz-k+1 do\n"
 }{MPLTEXT 1 0 40 "        if type(td[j,k],undefined) then\n" }
{MPLTEXT 1 0 62 "            td[j,k]:=(td[j+1,k-1]-td[j,k-1])/(z[j+k-1
